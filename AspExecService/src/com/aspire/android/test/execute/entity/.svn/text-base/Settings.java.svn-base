package com.aspire.android.test.execute.entity;

import com.thoughtworks.xstream.annotations.XStreamAlias;

/**
 * The class <code>Settings</code>
 *
 * @author likai
 * @version 1.0
 */

@XStreamAlias("settings")
public class Settings {

    @XStreamAlias("mobileProperties")
    protected MobileProperties mobileProperties;

    @XStreamAlias("systemProperties")
    protected SystemProperties systemProperties;
    
    @XStreamAlias("httpInterfaces")
    protected HttpInterfaceProperties httpInterfaces;
    
    @XStreamAlias("ftpInterfaces")
    protected FtpInterfacePropertiesList ftpInterfaces;
    /**
     * Getter of mobileProperties
     * @return the mobileProperties
     */
    public MobileProperties getMobileProperties() {
        return mobileProperties;
    }

    /**
     * Setter of mobileProperties
     * @param mobileProperties the mobileProperties to set
     */
    public void setMobileProperties(MobileProperties mobileProperties) {
        this.mobileProperties = mobileProperties;
    }

    /**
     * Getter of systemProperties
     * @return the systemProperties
     */
    public SystemProperties getSystemProperties() {
        return systemProperties;
    }

    /**
     * Setter of systemProperties
     * @param systemProperties the systemProperties to set
     */
    public void setSystemProperties(SystemProperties systemProperties) {
        this.systemProperties = systemProperties;
    }

    /**
     * Getter of httpInterfaces
     * @return the httpInterfaces
     */
    public HttpInterfaceProperties getHttpInterfaces() {
        return httpInterfaces;
    }

    /**
     * Setter of httpInterfaces
     * @param httpInterfaces the httpInterfaces to set
     */
    public void setHttpInterfaces(HttpInterfaceProperties httpInterfaces) {
        this.httpInterfaces = httpInterfaces;
    }

    /**
     * Getter of ftpInterfaces
     * @return the ftpInterfaces
     */
    public FtpInterfacePropertiesList getFtpInterfaces() {
        return ftpInterfaces;
    }

    /**
     * Setter of ftpInterfaces
     * @param ftpInterfaces the ftpInterfaces to set
     */
    public void setFtpInterfaces(FtpInterfacePropertiesList ftpInterfaces) {
        this.ftpInterfaces = ftpInterfaces;
    }

}
