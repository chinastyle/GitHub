package com.aspire.android.test.execute.entity;

import java.util.List;

import com.aspire.android.test.execute.ContentValues;

/**
 * The class <code>ExecuteSet</code>
 *
 * @author linjunsui
 * @version 1.0
 */
public class ExecuteSet {

    /**
     * caseList;
     */
    List<ContentValues> caseList;
    
    /**
     * environment setting
     */
    protected ContentValues setting;
    
    /**
     * environment setting
     */
    protected ContentValues globalVariables;

    /**
     * Constructor
     * @param caseList
     * @param setting
     * @param globalVariables
     */
    public ExecuteSet(List<ContentValues> caseList, ContentValues setting, ContentValues globalVariables) {
        super();
        this.caseList = caseList;
        this.setting = setting;
        this.globalVariables = globalVariables;
    }

    /**
     * Getter of caseList
     * @return the caseList
     */
    public List<ContentValues> getCaseList() {
        return caseList;
    }

    /**
     * Setter of caseList
     * @param caseList the caseList to set
     */
    public void setCaseList(List<ContentValues> caseList) {
        this.caseList = caseList;
    }

    /**
     * Getter of setting
     * @return the setting
     */
    public ContentValues getSetting() {
        return setting;
    }

    /**
     * Setter of setting
     * @param setting the setting to set
     */
    public void setSetting(ContentValues setting) {
        this.setting = setting;
    }

    /**
     * Getter of globalVariables
     * @return the globalVariables
     */
    public ContentValues getGlobalVariables() {
        return globalVariables;
    }

    /**
     * Setter of globalVariables
     * @param globalVariables the globalVariables to set
     */
    public void setGlobalVariables(ContentValues globalVariables) {
        this.globalVariables = globalVariables;
    }
}
