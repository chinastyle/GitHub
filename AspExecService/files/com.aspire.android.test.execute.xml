<?xml version="1.0" encoding="utf-8"?>
<settings>
  <!--本机配置信息-->
  <mobileProperties>
  	<!--城市编码-->
    <cityCode>7550</cityCode>
  	<!--设备名称-->
    <deviceName>HTC</deviceName>
  	<!--终端类型-->
    <deviceType>HTCG7</deviceType>
  	<!--IMEI-->
    <imei>864449500066302</imei>
  	<!--电话号码-->
    <mobileNum>18786000973</mobileNum>
  	<!--省份编码-->
    <provinceCode>200</provinceCode>
  	<!--厂商编码-->
    <companyCode>szzw</companyCode>
  	<!--测试人员-->
    <tester>luqj</tester>
  </mobileProperties>
  <!--系统参数设置-->
  <systemProperties>
  	<!--拨测方式；1：立即上次；2：定时上传；3：手动上传-->
    <uploadResultType>1</uploadResultType>
    <!--性能数据采集频率，单位：秒-->
    <dataCollectionFreq>60</dataCollectionFreq>
    <!--sdcard卡主目录-->
    <dataParentPath>/sdcard/aspire/es/</dataParentPath>
    <!--短信控制电话号码-->
    <smsControlNum>18666668888</smsControlNum>
    <!--连接开关-->
    <connectSwitch>true</connectSwitch>
  </systemProperties>
  <!--http接口设置-->
  <httpInterfaces>
  	<!--设备告警地址-->
    <deviceAlarmUrl>http://10.1.5.165:9090/sqmp/DeviceAlarm.action</deviceAlarmUrl>
  	<!--设备注册地址-->
    <deviceRegisterUrl>http://10.1.5.165:9090/sqmp/DeviceRegister.action</deviceRegisterUrl>
  	<!--设备状态更新间隔，单位：秒，-1表示不更新-->
    <deviceStatusInterval>60</deviceStatusInterval>
  	<!--设备状态更新地址-->
    <deviceStatusUrl>http://10.1.5.165:9090/sqmp/DeviceStatus.action</deviceStatusUrl>
  	<!--获取密钥的登录密码-->
    <pwdDownloadPwd>password</pwdDownloadPwd>
  	<!--获取密钥的地址，注：此链接地址不可带用户名，密码，日期-->
    <pwdDownloadUrl>http://10.1.5.165:9090/sqmp/generateKey/</pwdDownloadUrl>
  	<!--获取密钥的登录用户名-->
    <pwdDownloadUserName>username</pwdDownloadUserName>
  	<!--下载任务的时间间隔，单位：秒，-1表示不更新-->
    <taskDownloadInterval>60</taskDownloadInterval>
  	<!--下载任务的地址-->
    <taskDownloadUrl>http://10.1.5.165:9090/sqmp/TaskQuery.action</taskDownloadUrl>
  	<!--查询脚本更新地址-->
    <testScriptQueryUrl>http://10.1.5.165:9090/sqmp/ScriptQuery.action</testScriptQueryUrl>
  </httpInterfaces>
  <!--ftp相关属性设置-->
  <ftpInterfaces>
  <!--业务指标FTP设置-->
	  <serviceKeysFtpProperties>
	  	<!--ftp的ip地址-->
	    <ftpIp>10.1.5.163</ftpIp>
	  	<!--ftp上的路径-->
	    <ftpPath>sqmp/szzw/service_key</ftpPath>
	  	<!--ftp的端口号-->
	    <ftpPort>21</ftpPort>
	  	<!--ftp的密码-->
	    <ftpPwd>ftpsvr</ftpPwd>
	  	<!--ftp的用户名-->
	    <ftpUserName>ftpsvr</ftpUserName>
	  </serviceKeysFtpProperties>
	  <!--拨测脚本FTP设置-->
	  <testScriptFtpProperties>
	  	<!--脚本下载时间间隔，单位：秒，-1表示不更新-->
	    <ftpDownloadInterval>60</ftpDownloadInterval>
	  	<!--ftp的ip地址-->
	    <ftpIp>10.1.5.163</ftpIp>
	  	<!--ftp上的路径-->
	    <ftpPath>sqmp/szzw/test_script</ftpPath>
	  	<!--ftp的端口号-->
	    <ftpPort>21</ftpPort>
	  	<!--ftp的密码-->
	    <ftpPwd>ftpsvr</ftpPwd>
	  	<!--ftp的用户名-->
	    <ftpUserName>ftpsvr</ftpUserName>
	  </testScriptFtpProperties>
	  <!--拨测结果FTP设置-->
	  <testResultFtpProperties>
	  	<!--下载拨测结果响应文件的时间间隔，单位：秒，-1表示不更新-->
	    <ftpDownloadInterval>60</ftpDownloadInterval>
	  	<!--ftp的ip地址-->
	    <ftpIp>10.1.5.163</ftpIp>
	  	<!--ftp上的路径-->
	    <ftpPath>sqmp/szzw/test_result</ftpPath>
	  	<!--ftp的端口号-->
	    <ftpPort>21</ftpPort>
	  	<!--ftp的密码-->
	    <ftpPwd>ftpsvr</ftpPwd>
	  	<!--上传拨测结果文件的时间间隔，单位：秒，-1表示不更新-->
	    <ftpUploadInterval>60</ftpUploadInterval>
	  	<!--ftp的用户名-->
	    <ftpUserName>ftpsvr</ftpUserName>
	  </testResultFtpProperties>
	  <!--设备更新FTP设置-->
	  <versionFtpProperties>
	  	<!--ftp的ip地址-->
	    <ftpIp>10.1.5.163</ftpIp>
	  	<!--ftp上的路径-->
	    <ftpPath>sqmp/szzw/version</ftpPath>
	  	<!--ftp的端口号-->
	    <ftpPort>21</ftpPort>
	  	<!--ftp的密码-->
	    <ftpPwd>ftpsvr</ftpPwd>
	  	<!--ftp的用户名-->
	    <ftpUserName>ftpsvr</ftpUserName>
	  </versionFtpProperties>
	  <!-- 设置ftp数据交互超时时间 -->
	  <ftpDataTimeOut>60</ftpDataTimeOut>
  </ftpInterfaces>
</settings>