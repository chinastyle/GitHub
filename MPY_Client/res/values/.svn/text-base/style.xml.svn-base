<?xml version="1.0" encoding="utf-8"?>
<resources>
	<style name="titile_txt_size">
		<item name="android:textSize">16sp</item>
		<item name="android:textColor">#ff3e5162</item>
	</style>
	<style name="normal_btn">
		<item name="android:textSize">18sp</item>
		<item name="android:textColor">@android:color/white</item>
		<item name="android:background">@drawable/button_bg</item>
		<item name="android:gravity">center</item>
	</style>
	<style name="title_bar_text">
		<item name="android:textSize">19sp</item>	
		<item name="android:textColor">@android:color/white</item>	
	</style>
	
	<style name="loading" parent="@android:style/Widget.ProgressBar.Large">
        <item name="android:indeterminateDrawable">@anim/loadingpng</item>
    </style>
    <style name="contact_bottom_bar_tab" parent="title_bar_text">
    	<item name="android:textColor">#0d5ea9</item>
    </style>
	<style name="dialoglist_item">
		<item name="android:textSize">19.0sp</item>
		<item name="android:background">@drawable/list_item_select</item>
		<item name="android:textColor">@android:color/black</item>
		<item name="android:gravity">center</item>
	</style>
</resources>
