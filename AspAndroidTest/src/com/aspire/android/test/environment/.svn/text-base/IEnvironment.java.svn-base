package com.aspire.android.test.environment;

import com.aspire.android.test.execute.ContentValues;

public interface IEnvironment {

    /**
     * testService
     * 
     * @return
     */
    public IDeviceEntity getDeviceEntity();

    /**
     * @return the setting
     */
    public ContentValues getSetting();

    /**
     * @return the Global Variable
     */
    public ContentValues getGlobalVariables();

    /**
     * get the params
     */
    ContentValues getParams();

    /**
     * save the resultMap
     */
    void saveResult(ContentValues resultMap);

    /**
     * get mode
     * 
     * @return
     */
    String getMode();

    /**
     * get debug params
     * 
     * @return
     */
    ContentValues getDebugParams();

}
