package com.braver.gohome;

/**
 * The class <code>ZuoWei</code>
 * 
 * @author Feng OuYang
 * @version 1.0
 */
public class ZuoWei {

	private String name;

	private String zh_name;

	/**
	 * @return the name
	 */
	public String getName() {
		return name;
	}

	/**
	 * @param name
	 *            the name to set
	 */
	public void setName(String name) {
		this.name = name;
	}

	/**
	 * @return the zh_name
	 */
	public String getZh_name() {
		return zh_name;
	}

	/**
	 * @param zh_name
	 *            the zh_name to set
	 */
	public void setZh_name(String zh_name) {
		this.zh_name = zh_name;
	}

	public ZuoWei() {
	}

	/**
	 * @param name
	 * @param zh_name
	 */
	public ZuoWei(String name, String zh_name) {
		super();
		this.name = name;
		this.zh_name = zh_name;
	}

}
