package com.braver.gohome.task;

/**
 * The class <code>TaskStatus</code> 任务状态
 * 
 * @author Feng OuYang
 * @version 1.0
 */
public final class TaskStatus {

	/**
	 * 任务已提交
	 */
	public static final long SUBMIT = 0x1;
	/**
	 * 开如执行任务
	 */
	public static final long START = 0x2;
	/**
	 * 任务执行中
	 */
	public static final long DOING = 0x3;
	/**
	 * 任务暂停
	 */
	public static final long PAUSE = 0x4;
	/**
	 * 任务执行完成
	 */
	public static final long FINISH = 0x5;

	/**
	 * 任务失败
	 */
	public static final long FAIL = 0x6;

	/**
	 * 任务取消
	 */
	public static final long CANCELED = -0x1;

}
